  </div>
</div>

<%- contentFor('body') %>
<!--
<div class="jumbotron text-center">
  <h2>Waracle Devices</h2>
</div>
-->
<div class="panel panel-default">
  <!-- Default panel contents -->
  <div class="panel-heading">Waracle Devices</div>
  <div class="panel-body">
    <table class="table table-hover">
      <thead>
        <tr>
          <th>Device Name</th>
          <th>OS</th>
          <th>Version</th>
          <th>Location</th>
        </tr>
      </thead>
      <tbody>
        <% for (item of items) { %>
        <tr>
          <td><%= item.DeviceName %></td>
          <td><%= item.OS %></td>
          <td><%= item.Version %></td>
          <td><%= item.CurrentLocation %></td>
        </tr>
        <% } %>
      </tbody>
    </table>
  </div>
</div>
